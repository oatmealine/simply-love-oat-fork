		<Layer 
			File="@FUCK_EXE and 'SimplyLove' or 'SimplyOpen'" 
			InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" 
			OnCommand="%function(self) self:zoom(.9/(FUCK_EXE and 1 or 2)) if love == 0 then self:diffusealpha(0) self:linear(0.4) self:diffusealpha(1) end end"
			OffCommand="%TitleMenuOut"
		/>
		<Layer 
			File="SimplyLove overlay" 
			InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-5" 
			OnCommand="%function(self) self:zoom(.9) if love == 0 then self:diffusealpha(0) self:linear(0.4) self:diffusealpha(1) end self:queuecommand('Update') end"
			
		UpdateCommand="%function(self)
		    -- thank you bob for not working
		    self:accelerate(1)
		    self:y(SCREEN_CENTER_Y + 0)
		    self:decelerate(1)
		    self:y(SCREEN_CENTER_Y + 5)
		    self:accelerate(1)
		    self:y(SCREEN_CENTER_Y + 0)
		    self:decelerate(1)
		    self:y(SCREEN_CENTER_Y - 5)
		    self:queuecommand('Update')
		end"
			OffCommand="%TitleMenuOut"
		/>
